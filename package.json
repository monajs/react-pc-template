{
  "name": "react-pc-template",
  "version": "1.0.0",
  "description": "react pc projectp",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=dev && npm run config && node ./build/webpack.dev.js",
    "build:sit": "export NODE_ENV=sit && npm run build:common",
    "build:pre": "export NODE_ENV=pre && npm run build:common",
    "build:production": "export NODE_ENV=production && npm run build:common",
    "build:common": "npm run config && node ./build/webpack.build.js",
    "config": "node ./build/scripts/genConfig.js",
    "proxy": "node ./build/scripts/proxy.js",
    "lint": "eslint --fix --ext .js,.jsx src && stylelint ./src/style/*.less ./src/views/**/*.less --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
